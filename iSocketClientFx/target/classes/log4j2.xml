<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="Output" target="SYSTEM_OUT">
            <PatternLayout
                pattern="[%-6level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="all" additivity="true">
            <AppenderRef ref="Output" />
        </Root>
    </Loggers>
</Configuration>